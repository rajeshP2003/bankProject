<!--						<id>jsp-precompile</id>-->
<!--						<phase>generate-sources</phase>-->
<!--						<configuration>-->
<!--							<fragments>src/main/resources/WEB-INF/jsp/*.jsp</fragments>-->
<!--							<targetDir>target/classes</targetDir>-->
<!--							<compilerSourceVM>17</compilerSourceVM>-->
<!--							<compilerTargetVM>17</compilerTargetVM>-->
<!--						</configuration>-->
<!--					</execution>-->
<!--				</executions>-->
<!--			</plugin>-->

		</plugins>
	</build>

</project>
